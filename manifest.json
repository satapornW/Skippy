{
	"name": "Help Skip Extensions",
	"description" : "This extension helps you click the skip button on youtube.",
	"version" : "0.1",
	"manifest_version" : 2,
	"permissions": ["activeTab"],
	"icons": {
        "128": "iconSkippy.png"
    },
	"content_scripts" : [
		{
		"matches" : ["https://*.youtube.com/*"],
		"js" : ["background.js"]
		}
	]

}